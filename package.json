{
  "name": "@zakirkun/seblak",
  "version": "0.1.0",
  "description": "A lightweight React state management library",
  "type": "module",
  "main": "./dist/seblak.umd.js",
  "module": "./dist/seblak.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/seblak.es.js",
      "require": "./dist/seblak.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build --mode lib",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "react",
    "state-management",
    "hooks",
    "typescript",
    "lightweight",
    "reactive"
  ],
  "author": "Seblak Team",
  "license": "MIT",
  "homepage": "https://github.com/seblak-team/seblak#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seblak-team/seblak.git"
  },
  "bugs": {
    "url": "https://github.com/seblak-team/seblak/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.44.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0"
  }
}
